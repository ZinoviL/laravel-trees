{
  "name": "efureev/laravel-trees",
  "description": "Multi-Tree structures for Laravel",
  "license": "MIT",
  "keywords": [
    "php",
    "tree",
    "multi-tree",
    "laravel",
    "package"
  ],
  "authors": [
    {
      "name": "Eugene Fureev",
      "email": "fureev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0|^8.1|^8.2",
    "illuminate/database": "^8.8|^9.25|^10.0",
    "illuminate/events": "^8.80|^9.25|^10.0",
    "efureev/support": "^4.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5|^10.0",
    "orchestra/testbench": "^6.17|^7.22|8.0",
    "fakerphp/faker": "^1.20",
    "symfony/var-dumper": "^5.2|^6.1",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Fureev\\Trees\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fureev\\Trees\\Tests\\": "tests/",
      "Fureev\\Trees\\Tests\\Database\\Factories\\": "tests/database/factories"
    }
  },
  "scripts": {
    "phpcs": "@php ./vendor/bin/phpcs",
    "cs-fix": "@php ./vendor/bin/phpcbf",
    "test": "@php ./vendor/bin/phpunit --no-coverage --testdox",
    "test-cover": "@php ./vendor/bin/phpunit --coverage-text",
    "phpstan": "@php ./vendor/bin/phpstan analyze --ansi"
  }
}
